extends ../template

block variables
    - var title = 'Поиск'       

block content
    .main-container
        .breadcrumbs
            .breadcrumbs-link
                a(href="index.html") Главная
                img(src="assets/images/breadcrumbs.svg" alt="")
            .breadcrumbs-link
                a(href="search-result.html") Поиск
                img(src="assets/images/breadcrumbs.svg" alt="")
        .search-result-title
            .page-title
                span Результаты поиска по запросу “политика”
            .search-page-option
                select(id="search-page-option")             
                            option(value="relevant") По релевантности
                            option(value="new") По новизне
                            option(value="popular") По популярности
        .main-news.four-columns.search-result-news
            .main-news-left-block.grid-block
